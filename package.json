{
  "name": "ezpzthomasquez",
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build",
    "serve": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
